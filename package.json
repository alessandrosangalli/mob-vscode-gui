{
  "name": "mob-vscode-gui",
  "displayName": "Mob VSCode GUI",
  "description": "A mob.sh wrapper",
  "version": "0.0.1",
  "keywords": [
    "mob",
    "pairs",
    "ensemble"
  ],
  "publisher": "alessandrosangalli",
  "private": true,
  "engines": {
    "vscode": "^1.65.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alessandrosangalli/mob-vscode-gui.git"
  },
  "categories": [
    "Other",
    "SCM Providers"
  ],
  "activationEvents": [
    "onCommand:mob-vscode-gui.start",
    "onCommand:mob-vscode-gui.next",
    "onCommand:mob-vscode-gui.done",
    "onView:mobUtils"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "menus": {
      "view/title": [
        {
          "command": "mob-vscode-gui.start",
          "when": "view == mobUtils",
          "group": "navigation@1"
        },
        {
          "command": "mob-vscode-gui.next",
          "when": "view == mobUtils",
          "group": "navigation@2"
        },
        {
          "command": "mob-vscode-gui.done",
          "when": "view == mobUtils",
          "group": "navigation@3"
        }
      ]
    },
    "views": {
      "scm": [
        {
          "id": "mobUtils",
          "name": "Mob Utils"
        }
      ]
    },
    "commands": [
      {
        "command": "mob-vscode-gui.start",
        "title": "Start",
        "category": "MobVSCodeGui",
        "icon": "$(debug-start)"
      },
      {
        "command": "mob-vscode-gui.next",
        "title": "Next",
        "category": "MobVSCodeGui",
        "icon": "$(debug-step-over)"
      },
      {
        "command": "mob-vscode-gui.done",
        "title": "Done",
        "category": "MobVSCodeGui",
        "icon": "$(check)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.65.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5",
    "ts-loader": "^9.2.6",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "@vscode/test-electron": "^2.1.2"
  },
  "release": {
    "branches": "master",
    "verifyConditions": [
      "@semantic-release/github"
    ],
    "publish": [
      "@semantic-release/github"
    ],
    "success": [
      "@semantic-release/github"
    ],
    "fail": [
      "@semantic-release/github"
    ]
  }
}
